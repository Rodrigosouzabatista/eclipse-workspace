#include <iostream>
using namespace std;

// Exibe no terminal a msg "olá Mundo"

float soma (float a, float b){
	return a + b;
}

/**
 * Função principal
 * @return estado final do programa
 */

int main() {
	cout << "Olá Mundo!" << endl; // prints !!!Hello World!!!
	cout << soma (10,5.6) << endl;
	return 0;
}
